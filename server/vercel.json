{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    }
  ],
  "env": {
    "MONGO_DB": "your_mongodb_connection_string",
    "CLOUD_NAME": "your_cloud_name",
    "CLOUD_API_KEY": "your_cloud_api_key",
    "CLOUD_SECRET": "your_cloud_secret",
    "JWT_SECRET": "your_jwt_secret",
    "BASE_URL": "/api",
    "PORT": "5022"
  }
}
